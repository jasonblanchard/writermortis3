{
  "name": "react-app-base",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engine": "node 4.2.1",
  "scripts": {
    "test": "mocha --recursive --compilers js:babel-core/register",
    "test:watch": "npm test -- --watch",
    "build:server": "babel server_src/server -d server_build/server --experimental",
    "build": "webpack && npm run build:server",
    "server:web": "nodemon --exec babel-node -- server_src/server/index",
    "server:webpack": "webpack-dev-server --host localhost --port 8082 --content-base public --history-api-fallback --hot --inline --progress",
    "lint": "esw src/",
    "lint:watch": "npm run lint -- -w; exit 0",
    "start:dev": "npm run server:web & npm run server:webpack",
    "start": "NODE_ENV=production node server_build/server/index.js",
    "postinstall": "npm run build"
  },
  "author": "",
  "license": "ISC",
