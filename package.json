    "express": "^4.13.3",
    "express-handlebars": "^2.0.1",
    "immutable": "^3.7.6",
    "isomorphic-fetch": "^2.2.1",
    "markdown-it": "^5.1.0",
    "normalizr": "^2.0.0",
    "qajax": "^1.3.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-immutable-proptypes": "^1.5.1",
